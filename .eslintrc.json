{
    "root": true,
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "notModule",
        "strict": "safe"
    },
    "rules": {
        "no-multi-str": 0,
        "semi": 2,
        "no-multi-assign": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "no-plusplus": 0
    }
}
